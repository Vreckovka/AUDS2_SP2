<Window x:Class="SmestralnaPraca2.GeneratorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmestralnaPraca2"
        mc:Ignorable="d"
        Title="GeneratorWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Počet katastrálnych území" HorizontalAlignment="Center"/>
            <TextBox HorizontalAlignment="Center" Width="150" x:Name="PocetKatastrov"/>
            <Label Content="Počet nehnuteľností na územie" HorizontalAlignment="Center"/>
            <TextBox  HorizontalAlignment="Center" Width="150"  x:Name="PocetNehnutelnosti"/>
            <Button x:Name="GenerujBUtt" Content="Generuj" Margin="10" Width="150" Click="GenerujBUtt_Click" />
        </StackPanel>
    </Grid>
</Window>
